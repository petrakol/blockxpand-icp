type Holding = record {
  source: text;
  token: text;
  amount: text;
  status: text;
};

service: {
  "get_holdings": (principal) -> (vec Holding) query;
  "refresh_holdings": (principal) -> () update;
  "get_holdings_cert": (principal) -> (record {
    holdings: vec Holding;
    certificate: blob;
    witness: blob;
  }) query;
  "pools_graphql": (text) -> (text) query;
  "get_version": () -> (record {
    git_sha: text;
    build_time: text;
  }) query;
};
